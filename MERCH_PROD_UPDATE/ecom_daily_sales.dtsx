              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
dt.greg_date,
SUM(dmd.dmd_excl_buried/1000)AS dmd_excl_buried,
SUM(dmd.dmd_pos_excl_buried/1000)AS dmd_pos_excl_buried,
SUM(dmd.dmd_disc_excl_buried/1000)AS dmd_disc_excl_buried,
SUM(dmd.dmd_buried/1000) AS dmd_buried,
SUM(dmd.dmd_pos_buried/1000) AS dmd_pos_buried,
SUM(dmd.dmd_disc_buried/1000)AS dmd_disc_buried,
SUM(dmd.rtn_dlr/1000)AS rtn_dlr,
SUM(dmd.dmdu_excl_buried) AS dmdu_excl_buried,
SUM(dmd.dmdu_buried) AS dmdu_buried,
CASE 
	WHEN SUM(dmd.dmdu_excl_buried)+SUM(dmd.dmdu_buried)= 0 
		THEN NULL
	ELSE (SUM(dmd.dmd_excl_buried) + SUM(dmd.dmd_buried))/ (SUM(dmd.dmdu_excl_buried)+SUM(dmd.dmdu_buried)) END AS dmd_aur,
CASE 
	WHEN SUM(dmd.dmdu_excl_buried) =0
		THEN NULL
	ELSE SUM(dmd.dmd_excl_buried)/SUM(dmd.dmdu_excl_buried) END AS dmd_aur_excl_buried,
CASE
	WHEN SUM(dmd.dmdu_buried) =0
		THEN NULL
	ELSE SUM(dmd.dmd_buried)/SUM(dmd.dmdu_buried) END AS dmd_aur_buried,
SUM(ordcnt.ord_ttl) AS ord_ttl,	
SUM(ordcnt.ord_gwp) AS ord_gwp,
SUM(dmd.u_ttl) AS u_ttl,
SUM(dmd.u_gwp) AS u_gwp
FROM merch_prod.dbo.date_dim AS dt
INNER JOIN(
			SELECT ois.action_dt,
			--demand, excluding buried baubles
			SUM(CASE WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 0 
						THEN ois.dmd_dlr 
					ELSE 0 END) AS dmd_excl_buried,
			SUM(CASE 
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble= 0 AND pc.is_made_to_order = 1 
						THEN 0
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble= 0 AND pc.is_made_to_order = 0 
						THEN (ois.item_initial_price * dmdu) - ois.dmd_base
					ELSE 0 END) AS dmd_pos_excl_buried,
			SUM(CASE
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 0 
						THEN ois.dmd_cpn + ois.dmd_rwd + ois.dmd_emp
					ELSE 0 END)AS dmd_disc_excl_buried,
			--demand, buried baubles only
			SUM(CASE
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 1 
						THEN ois.dmd_dlr 
					ELSE 0 END)AS dmd_buried,
			0 AS dmd_pos_buried,
			SUM(CASE
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 1 
						THEN ois.dmd_cpn + ois.dmd_rwd + ois.dmd_emp
					ELSE 0 END)AS dmd_disc_buried,
			--returns
			SUM(CASE WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble=0 THEN ois.rtn_dlr ELSE 0 END)AS rtn_dlr,
			SUM(CASE 
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 0
						THEN ois.dmdu
					ELSE 0 END)AS dmdu_excl_buried,
			SUM(CASE
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0 AND pc.is_buried_bauble = 1 
						THEN ois.dmdu 
					ELSE 0 END)AS dmdu_buried,
			SUM(CASE
					WHEN ois.is_sales_revenue = 1 AND ois.is_bonus_bauble = 0
						THEN ois.dmdu
					ELSE 0 END) AS u_ttl,
			SUM(CASE
					WHEN ois.is_bonus_bauble = 1
						THEN ois.dmdu
					ELSE 0 END) AS u_gwp
			FROM merch_prod.dbo.order_items_summary AS ois
			INNER JOIN merch_prod.dbo.pid_control AS pc ON ois.product_id = pc.product_id
				AND ois.action_dt &gt;= pc.start_dt
				AND ois.action_dt &lt; pc.end_dt
			WHERE ois.is_ipr = 0
				AND pc.is_excluded_ecom = 0
			GROUP BY ois.action_dt
			)AS dmd ON dt.greg_date = dmd.action_dt
INNER JOIN	(
				SELECT action_dt,ord_ttl,ord_gwp
				FROM merch_prod.dbo.ecom_order_count
			)AS ordcnt ON dt.greg_date = ordcnt.action_dt
GROUP BY dt.greg_date
ORDER BY dt.greg_date</property>
