                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
case when ISDATE (left(receipt_at_date,19))=1 then convert(date, receipt_at_date,120) else null end as RECEIPT_DT,
case when ISDATE (left(put_away_at_date,19))=1 then convert(date, put_away_at_date,120) else null end as PUTAWAY_DT,
case when ISNUMERIC(ops_product_id)=1 then convert(int, ops_product_id) else null end as PRODUCT_ID,
convert(nvarchar(30), po_number) as PO_NUMBER,
case when ISNUMERIC(PO_Uploaded)=1 then convert(int, PO_Uploaded) else null end as PO_UPLOADED,
case when ISNUMERIC(Received)=1 then convert(int, Received) else null end as RECEIVED,
case when ISNUMERIC(Quality_Check)=1 then convert(int, Quality_Check) else null end as QUALITY_CHECK,
case when ISNUMERIC(Putaway)=1 then convert(int, Putaway) else null end as PUTAWAY,
case when ISNUMERIC(Adjust_by_LP)=1 then convert(int, Adjust_by_LP) else null end as ADJUST_BY_LP,
case when ISNUMERIC(Damaged)=1 then convert(int, Damaged) else null end as DAMAGED,
case when ISNUMERIC(QC_Hold)=1 then convert(int, QC_Hold) else null end as QC_HOLD
from STAGING.dbo.wms_purchase_order_reconcile
where ISNUMERIC(ops_product_id)=1
order by 3,4,1,2</property>
