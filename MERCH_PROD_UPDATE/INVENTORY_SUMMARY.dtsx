                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
convert(int, ops_product_id) as product_id,
convert(date, inventory_at,120) as INVENTORY_DT,
convert(nvarchar(50), coalesce(location_type,'')) as location_type,
convert(nvarchar(50), coalesce(reason_type,'')) as reason_type,
convert(nvarchar(50), coalesce(channel_type,'')) as channel_type,
sum(case when ISNUMERIC(qty_actual)=1 then convert(bigint, qty_actual) else 0 end) as qty_actual,
sum(case when ISNUMERIC(qty_unavailable)=1 then convert(int, qty_unavailable) else 0 end) as qty_unavailable,
sum(case when ISNUMERIC(qty_hold)=1 then convert(int, qty_hold) else 0 end) as qty_hold,
sum(case when ISNUMERIC(qty_damaged)=1 then convert(int, qty_damaged) else 0 end) as qty_damaged,
sum(case when ISNUMERIC(qty_rtv)=1 then convert(int, qty_rtv) else 0 end) as qty_rtv,
sum(case when ISNUMERIC(qty_fulfilling)=1 then convert(int, qty_fulfilling) else 0 end) as qty_fulfilling,
sum(case when ISNUMERIC(qty_sellable)=1 then convert(int, qty_sellable) else 0 end) as qty_sellable,
sum(case when ISNUMERIC(qty_order_pending)=1 then convert(int, qty_order_pending) else 0 end) as qty_order_pending,
sum(case when ISNUMERIC(qty_order_pending_shipped)=1 then convert(int, qty_order_pending_shipped) else 0 end) as qty_order_pending_shipped,
sum(case when ISNUMERIC(qty_pending_receiving)=1 then convert(int, qty_pending_receiving) else 0 end) as qty_pending_receiving,
case when isdate (left(last_receipt_at,19))=1 then convert(datetime, last_receipt_at,120) else null end as LAST_RECEIPT_DT
from staging.dbo.fact_inventory_summary_raw
where isnumeric(ops_product_id)=1 and isdate (left(inventory_at,19))=1
group by ops_product_id,inventory_at,location_type,reason_type,channel_type,last_receipt_at
order by convert(int, ops_product_id), convert(date, inventory_at,120),location_type,reason_type,channel_type</property>
